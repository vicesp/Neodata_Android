<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.android.my_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu_seleccion"
            android:label="Menú de Selección"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Configuraciones"
            android:label="Configuración de IP"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Sincronizar"
            android:label="Sincronizar Información"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Seleccion_lista"
            android:label="Control de Calidad"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Requisiciones"
            android:label="Requisiciones"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".Insumos"
            android:label="Insumos"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>