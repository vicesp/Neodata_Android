<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@android:color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="784dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">

    </android.support.v7.widget.Toolbar>

    <ImageButton
        android:id="@+id/ib_desde"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="-32dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/mes"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintLeft_toRightOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19" />

    <ImageButton
        android:id="@+id/ib_hasta"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/cale"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView20"
        app:layout_constraintLeft_toRightOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/textView20"
        app:layout_constraintVertical_bias="0.543" />

    <ImageButton
        android:id="@+id/ib_descargar"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="60dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/desc"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="132dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/upload"
        android:visibility="gone"
        app:layout_constraintLeft_toRightOf="@+id/ib_descargar"
        app:layout_constraintTop_toTopOf="@+id/ib_descargar" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginEnd="64dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/sync"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ib_descargar" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/del"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descargar"
        android:textSize="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/ib_descargar"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/ib_descargar"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/ib_descargar"
        app:layout_constraintHorizontal_bias="0.491"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enviar"
        android:textSize="22dp"
        android:visibility="gone"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton10"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton10"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton10"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sincronizar"
        android:textSize="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton11"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton11"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eliminar"
        android:textSize="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton12"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton12"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton12"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="300dp"
        android:layout_height="140dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/gsc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="20dp"
        android:text="Proyecto"
        android:textColor="@color/colorPrimary"
        android:textSize="22dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/ib_descargar"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="42dp"
        android:layout_marginTop="1dp"
        android:text="Desde"
        android:textColor="@color/colorPrimary"
        android:textSize="22dp"
        android:visibility="invisible"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintLeft_toLeftOf="@+id/textView18"
        app:layout_constraintRight_toRightOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="8dp"
        android:text="Hasta"
        android:textColor="@color/colorPrimary"
        android:textSize="22dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintLeft_toRightOf="@+id/editText2"
        app:layout_constraintTop_toTopOf="@+id/textView19"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/sp_desarrollo"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="0dp"
        android:background="#F2F2F2"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18"
        app:layout_constraintBottom_toBottomOf="@+id/textView18"
        android:layout_marginBottom="0dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textSize="15dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintLeft_toRightOf="@+id/ib_desde"
        app:layout_constraintTop_toTopOf="@+id/textView19"
        app:layout_constraintVertical_bias="0.75" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="-7dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textSize="15dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/textView20"
        app:layout_constraintLeft_toRightOf="@+id/ib_hasta"
        app:layout_constraintTop_toTopOf="@+id/textView20"
        app:layout_constraintVertical_bias="0.181" />

    <ImageButton
        android:id="@+id/ib_desc_preg"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/imageButton11"
        app:layout_constraintStart_toStartOf="@+id/imageButton11"
        app:layout_constraintTop_toBottomOf="@+id/textView24"
        app:srcCompat="@drawable/desc_preg" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:visibility="gone"
        android:text="Descarga\nPreguntas"
        app:layout_constraintEnd_toEndOf="@+id/ib_desc_preg"
        app:layout_constraintStart_toStartOf="@+id/ib_desc_preg"
        app:layout_constraintTop_toBottomOf="@+id/ib_desc_preg" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>